{
  "name": "tourze/doctrine-function-collection",
  "description": "Doctrine Function Collection",
  "type": "library",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^2.20 || ^3.0",
    "scienta/doctrine-json-functions": "^6.3.0",
    "beberlei/doctrineextensions": "^1.5.0"
  },
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\DoctrineFunctionCollection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\DoctrineFunctionCollection\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "phpstan": "phpstan analyse src -l 1"
  }
}